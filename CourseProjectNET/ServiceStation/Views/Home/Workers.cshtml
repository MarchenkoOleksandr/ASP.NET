@model IEnumerable<ServiceStation.Models.Worker>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Наши мастера</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.WorkerName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.WorkerName)
            </td>
        </tr>
    }

</table>
